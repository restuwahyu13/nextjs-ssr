{
	"name": "nextjs-ssr",
	"version": "1.0.0",
	"description": "Custom server using express server side rendering",
	"main": "./dist/server.js",
	"module": "dist/**/*",
	"scripts": {
		"cleanup:client": "rimraf client/.next",
		"cleanup:server": "rimraf dist",
		"client:build": "npm run cleanup:client && next build client",
		"server:build": "npm run cleanup:server && tsc --pretty",
		"dc": "cross-env NODE_ENV=development PORT=3000 next dev client",
		"ds": "cross-env NODE_ENV=server PORT=5000 tsnd --poll --respawn --watch src/**/*.{ts} src/server.ts",
		"start": "node dist/server.js",
		"build": "npm run client:build && npm run server:build"
	},
	"repository": {
		"type": "git",
		"url": "restuwahyu13"
	},
	"keywords": [
		"nextjs",
		"typescript"
	],
	"author": "restuwahyu13",
	"license": "MIT",
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.11.1",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.14.2",
		"@babel/plugin-syntax-typescript": "^7.12.13",
		"@babel/plugin-transform-async-to-generator": "^7.13.0",
		"@babel/plugin-transform-modules-commonjs": "^7.14.0",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-typescript": "^7.10.4",
		"@types/axios": "^0.14.0",
		"@types/body-parser": "^1.19.0",
		"@types/express": "^4.17.11",
		"@types/node": "^14.17.1",
		"@types/react": "^16.9.44",
		"@types/react-dom": "^16.9.8",
		"@types/rimraf": "^3.0.0",
		"core-js": "^3.13.0",
		"nodemon": "^2.0.4",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2",
		"ts-node": "^8.10.2",
		"ts-node-dev": "^1.1.6",
		"tslib": "^2.3.0",
		"typescript": "^3.9.7"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"body-parser": "^1.19.0",
		"bootstrap": "^5.0.1",
		"cross-env": "^7.0.2",
		"express": "^4.17.1",
		"next": "^9.5.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	}
}
