{
	"name": "nextjs-ssr",
	"version": "1.0.0",
	"description": "Custom server using express server side rendering",
	"main": "./dist/server.js",
	"module": "dist/**/*",
	"scripts": {
		"cleanup:client": "rimraf client/.next",
		"cleanup:server": "rimraf dist",
		"client:build": "npm run cleanup:client && next build client",
		"server:build": "npm run cleanup:server && tsc --pretty",
		"dc": "cross-env TYPE=client PORT=3000 nodemon",
		"ds": "cross-env TYPE=server PORT=5000 tsnd --poll --respawn --watch src/**/*.{ts} src/server.ts",
		"start": "node dist/server.js",
		"build": "npm run client:build && npm run server:build"
	},
	"repository": {
		"type": "git",
		"url": "restuwahyu13"
	},
	"keywords": [
		"nextjs",
		"typescript"
	],
	"author": "restuwahyu13",
	"license": "MIT",
